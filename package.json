{
  "name": "vue-wf-designer",
  "version": "1.1.9",
  "private": false,
  "description": "基于Vue2.x和ElementUI的流程设计器",
  "keywords": [
    "bpmn",
    "bpmn.js",
    "vue"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": {
    "name": "Wangyu",
    "email": "wybaby168@gmail.com"
  },
  "main": "package/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:wybaby168/bpmn-process-designer.git"
  },
  "scripts": {
    "demo": "vue-cli-service serve",
    "build": "vue-cli-service build --report",
    "build-lib": "vue-cli-service build --target lib --name vue-wf-designer --dest lib package/index.js"
  },
  "dependencies": {
    "bpmn-js": "^11.3.0",
    "bpmn-js-properties-panel": "^1.17.0",
    "bpmn-js-token-simulation": "^0.31.0",
    "element-ui": "^2.15.12",
    "core-js": "^3.8.1",
    "diagram-js-minimap": "^4.0.1",
    "highlight.js": "^10.5.0",
    "min-dash": "^3.5.2",
    "vue": "^2.7.14",
    "vue-router": "^3.6.5",
    "x2js": "^3.4.4",
    "xml-js": "^1.6.11",
    "axios": "^1.3.2",
    "camunda-bpmn-moddle": "^4.4.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-plugin-eslint": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "@vue/compiler-sfc": "^2.7.14",
    "vue-template-compiler": "^2.7.14",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "compression-webpack-plugin": "^6.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^6.2.2",
    "fs-extra": "^11.1.0",
    "sass": "^1.30.0",
    "sass-loader": "^8.0.2",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "terser-webpack-plugin": "^4.2.3",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-use-before-define": "off",
      "no-unused-vars": "off",
      "prettier/prettier": [
        "warn",
        {
          "useTabs": false,
          "singleQuote": false,
          "printWidth": 160,
          "semi": true,
          "trailingComma": "none",
          "bracketSpacing": true,
          "jsxBracketSameLine": true,
          "arrowParens": "avoid",
          "requirePragma": false,
          "proseWrap": "preserve"
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "Apache-2.0"
}
